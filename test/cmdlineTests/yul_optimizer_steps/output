object "C_6" {
    code {
        {
            mstore(64, 128)
            if callvalue() { revert(0, 0) }
            codecopy(0, dataoffset("C_6_deployed"), datasize("C_6_deployed"))
            return(0, datasize("C_6_deployed"))
        }
    }
    object "C_6_deployed" {
        code {
            {
                mstore(64, 128)
                if iszero(lt(calldatasize(), 4))
                {
                    mstore(0, 0)
                    calldatacopy(28, 0, 4)
                    let selector := mload(0)
                    pop(selector)
                }
                pop(iszero(calldatasize()))
                revert(0, 0)
            }
        }
    }
}
